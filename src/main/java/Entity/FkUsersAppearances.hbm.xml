<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Entity.FkUsersAppearances" table="fk_users_appearances" schema="yes">
        <composite-id mapped="true" class="Entity.FkUsersAppearancesPK">
            <key-property name="uId">
                <column name="UId" sql-type="int(11)"/>
            </key-property>
            <key-property name="aId">
                <column name="AId" sql-type="int(11)"/>
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>