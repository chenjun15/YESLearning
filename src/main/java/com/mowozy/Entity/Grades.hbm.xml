<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.mowozy.Entity.Grades" table="grades" schema="yes">
        <id name="gId">
            <column name="GId" sql-type="int(11)"/>
        </id>
        <property name="gDate">
            <column name="GDate" sql-type="datetime" not-null="true"/>
        </property>
        <property name="grade">
            <column name="Grade" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>