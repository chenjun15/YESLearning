<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.mowozy.Entity.FkUsersCourses" table="fk_users_courses" schema="yes">
        <composite-id mapped="true" class="com.mowozy.Entity.FkUsersCoursesPK">
            <key-property name="cId">
                <column name="CId" sql-type="int(11)"/>
            </key-property>
            <key-property name="uId">
                <column name="UId" sql-type="int(11)"/>
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>